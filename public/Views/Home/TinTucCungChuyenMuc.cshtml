@model List<Web_MVC.Models.News>

@if (Model != null)
{
    foreach (var tintuc in Model)
    {
        <span class="fa fa-caret-right" ></span> <a style="line-height:2" href="/Home/Detail/@tintuc.Id">@tintuc.Title <span style="font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;font-size:13px;">(@tintuc.Dateposted.Value.ToString("dd/MM/yyyy"))</span></a><br />
    }
}
